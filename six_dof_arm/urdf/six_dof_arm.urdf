<?xml version="1.0"?>
<robot name="fanuc" xmlns:xacro="http://ros.org/wiki/xacro">

  <!-- Base -->
  <link name="base"/>
  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/visual/base_link.stl"/>
      </geometry>
      <material name="grey">
        <color rgba="0.4 0.4 0.4 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/collision/base_link.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="base-base_link" type="fixed">
    <origin xyz="0 0 0.45" rpy="0 0 0"/>
    <parent link="base"/>
    <child link="base_link"/>
  </joint>

  <!-- Link 1 -->
  <link name="link_1">
    <visual>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/visual/link_1.stl"/>
      </geometry>
      <material name="yellow">
        <color rgba="0.96 0.76 0.13 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/collision/link_1.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint_1" type="revolute">
    <origin xyz="0 0 0.45" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="link_1"/>
    <axis xyz="0 0 1"/>
    <limit effort="0" lower="-3.14" upper="3.14" velocity="3.67"/>
  </joint>

  <!-- Link 2 -->
  <link name="link_2">
    <visual>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/visual/link_2.stl"/>
      </geometry>
      <material name="yellow"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/collision/link_2.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint_2" type="revolute">
    <origin xyz="0.15 0 0" rpy="0 0 0"/>
    <parent link="link_1"/>
    <child link="link_2"/>
    <axis xyz="0 1 0"/>
    <limit effort="0" lower="-1.57" upper="2.79" velocity="3.32"/>
  </joint>

  <!-- Link 3 -->
  <link name="link_3">
    <visual>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/visual/link_3.stl"/>
      </geometry>
      <material name="yellow"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/collision/link_3.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint_3" type="revolute">
    <origin xyz="0 0 0.6" rpy="0 0 0"/>
    <parent link="link_2"/>
    <child link="link_3"/>
    <axis xyz="0 -1 0"/>
    <limit effort="0" lower="-3.14" upper="4.61" velocity="3.67"/>
  </joint>

  <!-- Link 4 -->
  <link name="link_4">
    <visual>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/visual/link_4.stl"/>
      </geometry>
      <material name="yellow"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/collision/link_4.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint_4" type="revolute">
    <origin xyz="0 0 0.2" rpy="0 0 0"/>
    <parent link="link_3"/>
    <child link="link_4"/>
    <axis xyz="-1 0 0"/>
    <limit effort="0" lower="-3.31" upper="3.31" velocity="6.98"/>
  </joint>

  <!-- Link 5 -->
  <link name="link_5">
    <visual>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/visual/link_5.stl"/>
      </geometry>
      <material name="yellow"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/collision/link_5.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint_5" type="revolute">
    <origin xyz="0.64 0 0" rpy="0 0 0"/>
    <parent link="link_4"/>
    <child link="link_5"/>
    <axis xyz="0 -1 0"/>
    <limit effort="0" lower="-3.31" upper="3.31" velocity="6.98"/>
  </joint>

  <!-- Link 6 -->
  <link name="link_6">
    <visual>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/visual/link_6.stl"/>
      </geometry>
      <material name="dark_grey">
        <color rgba="0.15 0.15 0.15 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/collision/link_6.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint_6" type="revolute">
    <origin xyz="0.1 0 0" rpy="0 0 0"/>
    <parent link="link_5"/>
    <child link="link_6"/>
    <axis xyz="-1 0 0"/>
    <limit effort="0" lower="-6.28" upper="6.28" velocity="10.47"/>
  </joint>

  <!-- Tool & Gripper -->
  <link name="tool0"/>
  <joint name="joint_6-tool0" type="fixed">
    <origin xyz="0 0 0" rpy="3.1415926535 -1.570796327 0"/>
    <parent link="link_6"/>
    <child link="tool0"/>
  </joint>

  <link name="gripper">
    <visual>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/visual/hand.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/collision/hand.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="gripper_joint" type="fixed">
    <parent link="tool0"/>
    <child link="gripper"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <!-- Fingers -->
  <link name="gripper_leftfinger">
    <visual>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/visual/finger.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/collision/finger.stl"/>
      </geometry>
    </collision>
  </link>

  <link name="gripper_rightfinger">
    <visual>
      <origin rpy="0 0 3.14159265359" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/visual/finger.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 3.14159265359" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://six_dof_arm/meshes/collision/finger.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="gripper_finger_joint1" type="prismatic">
    <parent link="gripper"/>
    <child link="gripper_leftfinger"/>
    <origin xyz="0 0 0.0284" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="20" lower="0.0" upper="0.04" velocity="0.2"/>
  </joint>

  <joint name="gripper_finger_joint2" type="prismatic">
    <parent link="gripper"/>
    <child link="gripper_rightfinger"/>
    <origin xyz="0 0 0.0284" rpy="0 0 0"/>
    <axis xyz="0 -1 0"/>
    <limit effort="20" lower="0.0" upper="0.04" velocity="0.2"/>
    <mimic joint="gripper_finger_joint1"/>
  </joint>

</robot>
